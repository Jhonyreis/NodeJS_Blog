<%- include ('../../partials/header') %>
<%- include ('../../partials/navbar') %>
<div class="container">
  <table class="table table-striped table-light">
    <thead>
      <tr>
        <th>ID</th>
        <th>Título</th>
        <th>Slug</th>
        <th>Ações</th>
      </tr>
    </thead>
    <tbody>
      <% categories.forEach(category => { %>
        <tr>
          <td><%= category.id %></td>
          <td><%= category.title %></td>
          <td><%= category.slug %></td>
          <td></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</div>
<%- include ('../../partials/footer') %>